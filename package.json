{
  "name": "unocss-preset-material",
  "version": "0.0.1",
  "type": "module",
  "description": "A unocss preset for material design",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akinoccc/unocss-preset-material.git"
  },
  "main": "index.js",
  "keywords": [
    "unocss",
    "preset",
    "material",
    "design"
  ],
  "author": "akinoccc",
  "license": "ISC",
  "scripts": {
    "dev": "pnpm -C playground dev",
    "build": "tsup --format esm,cjs --clean --dts",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "unocss": "^0.61.5"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "unocss": "^0.61.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.7.0",
    "eslint-config-airbe": "1.0.0-beta.4",
    "eslint-plugin-vue": "^9.27.0",
    "lint-staged": "^15.2.7",
    "semantic-release": "^24.0.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "8.0.0-alpha.46",
    "vue-eslint-parser": "^9.4.3"
  },
  "packageManager": "pnpm@9.0.0",
  "lint-staged": {
    "*.{js,tx,jsx,tsx,vue,mjs}": [
      "npm run lint:fix",
      "git add ."
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "pnpm typecheck"
  }
}
